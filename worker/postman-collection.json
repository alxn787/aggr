{
  "info": {
    "name": "MemeCoin Aggregator API",
    "description": "Complete API collection for testing the real-time data aggregation service",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000",
      "type": "string"
    },
    {
      "key": "wsUrl",
      "value": "ws://localhost:8080",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/health",
          "host": ["{{baseUrl}}"],
          "path": ["health"]
        }
      },
      "response": []
    },
    {
      "name": "Get All Tokens (Default)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/api/tokens",
          "host": ["{{baseUrl}}"],
          "path": ["api", "tokens"]
        }
      },
      "response": []
    },
    {
      "name": "Get Tokens with Pagination",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/api/tokens?limit=5&cursor=0",
          "host": ["{{baseUrl}}"],
          "path": ["api", "tokens"],
          "query": [
            {
              "key": "limit",
              "value": "5"
            },
            {
              "key": "cursor",
              "value": "0"
            }
          ]
        }
      },
      "response": []
    },
    {
      "name": "Get Tokens - Sort by Volume",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/api/tokens?sortField=volume24h&sortDirection=desc&limit=10",
          "host": ["{{baseUrl}}"],
          "path": ["api", "tokens"],
          "query": [
            {
              "key": "sortField",
              "value": "volume24h"
            },
            {
              "key": "sortDirection",
              "value": "desc"
            },
            {
              "key": "limit",
              "value": "10"
            }
          ]
        }
      },
      "response": []
    },
    {
      "name": "Get Tokens - Sort by Price Change",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/api/tokens?sortField=priceChange24h&sortDirection=desc&limit=10",
          "host": ["{{baseUrl}}"],
          "path": ["api", "tokens"],
          "query": [
            {
              "key": "sortField",
              "value": "priceChange24h"
            },
            {
              "key": "sortDirection",
              "value": "desc"
            },
            {
              "key": "limit",
              "value": "10"
            }
          ]
        }
      },
      "response": []
    },
    {
      "name": "Get Tokens - Filter by Volume",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/api/tokens?minVolume=1000000&maxVolume=10000000",
          "host": ["{{baseUrl}}"],
          "path": ["api", "tokens"],
          "query": [
            {
              "key": "minVolume",
              "value": "1000000"
            },
            {
              "key": "maxVolume",
              "value": "10000000"
            }
          ]
        }
      },
      "response": []
    },
    {
      "name": "Get Tokens - Filter by Price Change",
      "request": {
        "method": "GET",
        "header": [],
