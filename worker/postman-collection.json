    {
      "name": "Refresh Token Data",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/refresh",
          "host": ["{{baseUrl}}"],
          "path": ["api", "refresh"]
        }
      },
      "response": []
