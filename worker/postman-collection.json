          "host": ["{{baseUrl}}"],
          "path": ["api", "tokens"],
          "query": [
            {
              "key": "minPriceChange",
              "value": "50"
            },
            {
              "key": "sortField",
              "value": "priceChange24h"
            },
            {
              "key": "sortDirection",
              "value": "desc"
            },
            {
              "key": "limit",
              "value": "10"
            }
          ]
        }
      },
      "response": []
    }
  ]
}
